{
	"name": "regexworkbench",
	"displayName": "Regex Workbench",
	"description": "A workbench to design, test, and experiment with regular expressions",
	"keywords": [
		"regex",
		"regexes",
		"regexp",
		"workbench",
		"regular expression"
	],
	"icon": "icon.png",
	"publisher": "stephen-riley",
	"repository": {
		"type": "git",
		"url": "https://github.com/stephen-riley/regexworkbench"
	},
	"version": "2.0.5",
	"preview": false,
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*",
		"onCommand:regexworkbench.start",
		"onWebviewPanel:regexworkbench"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "regexworkbench.start",
				"title": "Open regular expression workbench",
				"category": "Regex Workbench"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.17.28",
		"@types/vscode": "^1.48.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"tslint": "^5.20.1",
		"typescript": "^3.9.7",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"@stephen-riley/pcre2-wasm": "^1.2.4"
	}
}